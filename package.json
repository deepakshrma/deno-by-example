{
  "name": "deno-by-example-1",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "rm -rf .docusaurus docs && docusaurus build --out-dir docs",
    "swizzle": "docusaurus swizzle",
    "doc": "deno run --allow-read --allow-write scripts/run.ts g",
    "deploy": "GIT_USER=deepakshrma CURRENT_BRANCH=master USE_SSH=true yarn run docusaurus deploy",
    "prettify": "prettier src/**/*.js --write && prettier website/*.md --write"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.56",
    "@docusaurus/preset-classic": "^2.0.0-alpha.56",
    "classnames": "^2.2.6",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build && git add docs"
    }
  },
  "lint-staged": {
    "*.(js|html|css|jsx|md|mdx)": [
      "prettier --write"
    ]
  },
  "devDependencies": {},
  "devDependencie": {
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5"
  }
}
